    "pinia": "^2.0.32",
    "sweetalert2": "^11.7.3",
    "v-calendar": "^3.0.3",
    "vee-validate": "^4.8.6",
    "video.js": "^8.3.0",
    "vue": "^3.2.47",
    "vue-loading-overlay": "^6.0.3",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0"
