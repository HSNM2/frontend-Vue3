    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "sass": "^1.62.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
